{
  "[go.mod]": {
    "editor.defaultFormatter": "golang.go-nightly"
  },
  "[go]": {
    "editor.bracketPairColorization.enabled": false,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.defaultFormatter": "golang.go-nightly",
    "editor.formatOnSave": true,
    "editor.insertSpaces": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.console.fontFamily": "Cartograph CF", // OperatorMono Nerd Font Book
  "debug.console.fontSize": 20,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeLensFontFamily": "Gintronic Nerd Font Mono", // OperatorMono Nerd Font Book
  "editor.cursorStyle": "line",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Gintronic Nerd Font Mono", // Operator SSm
  "editor.fontLigatures": "'ss05', 'calt', 'liga', 'zero'",
  "editor.fontSize": 30,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.indentation": true,
  "editor.insertSpaces": false,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "cmd": "golines -m 80 -t 2 --no-chain-split-dots --base-formatter gofumpt ${file} | gci write -s standard -s default -s blank -s dot ${file}",
        "match": "\\.go$"
      }
    ]
  },
  "emojisense.languages": {
    "git-commit": true,
    "javascript": true,
    "markdown": {
      "markupCompletionsEnabled": true,
      "unicodeCompletionsEnabled": false
    },
    "plaintext": {
      "emojiDecoratorsEnabled": false,
      "markupCompletionsEnabled": false
    },
    "typescript": true
  },
  "emojisense.unicodeCompletionsEnabled": true,
  "explorer.confirmDelete": false,
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "gitlens.blame.ignoreWhitespace": true,
  "gitlens.telemetry.enabled": false,
  "go.buildOnSave": "off",
  "go.coverageOptions": "showUncoveredCodeOnly",
  "go.coverOnSingleTest": true,
  "go.coverOnSingleTestFile": true,
  "go.formatTool": "gofumpt",
  "go.lintFlags": ["-c", "~/.golangci.yaml"],
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "gopls": {
    "formatting.gofumpt": true
  },
  "indentRainbow.excludedLanguages": [
    "plaintext",
    "json",
    "jsonc",
    "JSON",
    "JSON with Comments",
    "Markdown"
  ],
  "javascript.preferences.quoteStyle": "single",
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.maxItemsComputed": 25000,
  "languageToolLinter.external.url": "http://localhost:8010",
  "languageToolLinter.languageTool.ignoredWordsGlobal": ["&nbsp", "mongodb", "pritunl"],
  "markdown.preview.fontSize": 24,
  "prettier.configPath": "~/.prettierrc",
  "prettier.documentSelectors": ["*.{tsx?, jsx?, json}"],
  "prettier.endOfLine": "lf",
  "prettier.trailingComma": "es5",
  "prettier.useTabs": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "kitty.app",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.fontFamily": "Gintronic Nerd Font Mono", // OperatorMono Nerd Font Book
  "terminal.integrated.fontSize": 24,
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "todo-tree.filtering.excludeGlobs": ["**/node_modules", "**/ios", "**/vendor"],
  "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "XXX", "[ ]", "[x]", "INFO"],
  "todo-tree.highlights.customHighlight": {
    "FIX": {
      "background": "#E9436F",
      "icon": "info",
      "iconColour": "#E9436F",
      "type": "tag-and-comment"
    },
    "FIXME": {
      "background": "#FDF0ED",
      "icon": "flame",
      "iconColour": "#FDF0ED",
      "type": "line"
    },
    "INFO": {
      "background": "#09F7A0",
      "icon": "info",
      "iconColour": "#09F7A0",
      "type": "tag-and-comment"
    },
    "TODO": {
      "background": "#FAB28E",
      "icon": "clock",
      "iconColour": "#FAB28E",
      "type": "line"
    }
  },
  "todo-tree.highlights.defaultHighlight": {
    "background": "yellow",
    "foreground": "black",
    "iconColour": "lime",
    "type": "text"
  },
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "typescript.preferences.quoteStyle": "single",
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": 2,
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f",
    "editorBracketHighlight.foreground1": "#6a51e6",
    "editorBracketHighlight.foreground2": "#9a86fd",
    "editorBracketHighlight.foreground3": "#c4b9fe",
    "editorBracketHighlight.foreground4": "#ffa142",
    "editorBracketHighlight.foreground5": "#ffb870",
    "editorBracketHighlight.foreground6": "#eeebff",
    "editorBracketHighlight.unexpectedBracket.foreground": "#db6165"
  },
  "workbench.colorTheme": "Base2Tone_EveningDark",
  "workbench.iconTheme": "material-icon-theme"
}
